<main class=" body col-md-9 ml-sm-auto col-lg-10 px-md-4 py-4">


    <h1>All Users</h1>
    <table class="table table-dark table-striped-columns">
      <thead>
        <tr>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
          <th>Password</th>
          <th>Edit</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
          <tr>
            <td>{{this.fname}}</td>
            <td>{{this.lname}}</td>
            <td>{{this.email}}</td>
            <td>{{this.psw}}</td>
            <td><a href="/userlist/edit/{{this._id}}" style="color: white;">Edit</a></td>
            <td><a href="/userlist/delete/{{this._id}}" style="color: white;">Delete</a></td>
          </tr>
        {{/each}}
      </tbody>
    </table>
</main>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
